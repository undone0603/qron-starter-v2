{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "functions": {
    "app/api/generate/route.ts": {
      "maxDuration": 60
    },
    "app/api/video/route.ts": {
      "maxDuration": 300
    },
    "app/api/mint/route.ts": {
      "maxDuration": 30
    }
  }
}
